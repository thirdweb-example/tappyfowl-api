{
  "name": "@thirdweb-example/tappy-fowl-api",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/server.cjs",
  "module": "./dist/server.mjs",
  "scripts": {
    "build": "pkgroll",
    "predev": "pkgroll",
    "dev": "node dist/server.js",
    "start": "node dist/server.js",
    "docker:build": "docker build -t ghcr.io/thirdweb-example/tappyfowl-api:latest .",
    "docker:run": "docker run --rm -p 3000:3000 ghcr.io/thirdweb-example/tappyfowl-api:latest",
    "docker:push": "docker push ghcr.io/thirdweb-example/tappyfowl-api:latest"
  },
  "exports": {
    "require": {
      "default": "./dist/server.cjs"
    },
    "import": {
      "default": "./dist/server.mjs"
    }
  },
  "bin": "./dist/server.js",
  "dependencies": {
    "cookie-parser": "~1.4.6",
    "debug": "4.3.6",
    "express": "4.19.2",
    "http-errors": "~2.0.0",
    "morgan": "~1.10.0",
    "viem": "^2.20.0"
  },
  "devDependencies": {
    "@thirdweb-dev/engine": "^0.0.12",
    "@types/cookie-parser": "^1.4.7",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.5.0",
    "pkgroll": "^2.1.1",
    "typescript": "^5.5.4"
  }
}
